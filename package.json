{
  "name": "vendor-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write . && git add .",
    "pre-commit": "lint-staged",
    "lint:strict": "eslint 'src --ext .ts --max-warnings=0",
    "typecheck:strict": "tsc --noEmit --strict",
    "audit": "npm run lint:strict && npm run typecheck:strict",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BIBUAIN-UNIQUE-MULTIPURPOSE-LIMITED/vendor-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BIBUAIN-UNIQUE-MULTIPURPOSE-LIMITED/vendor-backend/issues"
  },
  "homepage": "https://github.com/BIBUAIN-UNIQUE-MULTIPURPOSE-LIMITED/vendor-backend#readme",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.29.0",
    "express": "^5.1.0",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "npm": "^11.4.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "eslint-plugin-sonarjs": "^3.0.3"
  }
}
